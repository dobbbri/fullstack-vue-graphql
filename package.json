{
  "name": "fullstack-vue-graphql-starter",
  "version": "1.0.0",
  "description": "Starter for Full-Stack Vue-GraphQL-Apollo Projects",
  "author": "Reed Barger",
  "scripts": {
    "client": "cd src/client && npm run serve",
    "dev": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\"",
    "server": "nodemon src/server.js",
    "start": "node src/server.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-rc.1",
    "apollo-server": "^2.3.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "jsonwebtoken": "^8.3.0",
    "md5": "^2.2.1",
    "moment": "^2.23.0",
    "mongoose": "^5.4.0",
    "node-sass": "^4.11.0",
    "vuetify": "^1.3.15"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "vue-cli-plugin-vuetify": "^0.4.6"
  },
  "keywords": [],
  "license": "ISC",
  "main": "src/server.js"
}
